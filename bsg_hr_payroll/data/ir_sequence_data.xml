<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_hr_contract" model="ir.sequence">
            <field name="name">HR contract Sequence</field>
            <field name="code">hr.contract</field>
            <field name="prefix">HRCON-%(y)s</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
            <field name="implementation">no_gap</field>
        </record>
        </data>
        <data noupdate="0">
        <record id="sequence_hr_employee_code" model="ir.sequence">
            <field name="name">HR Employee Code Sequence</field>
            <field name="code">hr.employee</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
            <field name="padding">5</field>
            <field name="implementation">no_gap</field>
        </record>
    </data>
</odoo>