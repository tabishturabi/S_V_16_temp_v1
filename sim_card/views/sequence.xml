<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <record id="seq_sim_card_request_seq" model="ir.sequence">
            <field name="name">Sim Card Request</field>
            <field name="code">sim.card.request</field>
            <field name="prefix">%(y)s%(month)s</field>
            <field name="padding">3</field>
        </record>

        <record id="seq_sim_card_delivery_seq" model="ir.sequence">
            <field name="name">Sim Card Delivery</field>
            <field name="code">sim.card.delivery</field>
            <field name="prefix">%(y)s%(month)s</field>
            <field name="padding">3</field>
        </record>

        <record id="seq_sim_card_receipt_seq" model="ir.sequence">
