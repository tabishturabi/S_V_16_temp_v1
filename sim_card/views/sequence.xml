            <field name="code">sim.card.receipt</field>
            <field name="prefix">%(y)s%(month)s</field>
            <field name="padding">3</field>
        </record>

        <record id="seq_sim_card_upgrade_seq" model="ir.sequence">
            <field name="name">Sim Card Upgrade</field>
            <field name="code">upgrade.request</field>
            <field name="prefix">%(y)s%(month)s</field>
            <field name="padding">3</field>
        </record>

        <record id="seq_sim_card_lost_seq" model="ir.sequence">
            <field name="name">Lost Sim Card Request</field>
            <field name="code">lost.request</field>
            <field name="prefix">%(y)s%(month)s</field>
