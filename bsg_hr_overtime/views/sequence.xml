<odoo>
    <data noupdate="0">
        <record model="ir.sequence" id="overtime_request_sequence">
          <field name="name">name</field>
          <field name="code">orsequencecode</field>
            <field name="prefix">%(y)s%(month)s</field>
          <field name="padding">4</field>
          <field name="number_increment">1</field>
          <field name="implementation">no_gap</field>
      </record>
      <record model="ir.sequence" id="my_overtime_request_sequence">
          <field name="name">name</field>
          <field name="code">morsequencecode</field>
            <field name="prefix">%(y)s%(month)s</field>
          <field name="padding">4</field>
          <field name="number_increment">1</field>
          <field name="implementation">no_gap</field>
      </record>

      <record model="ir.sequence" id="multi_overtime_request_sequence">
          <field name="name">name</field>
          <field name="code">multisequencecode</field>
            <field name="prefix">%(y)s%(month)s</field>
          <field name="padding">5</field>
          <field name="number_increment">1</field>
          <field name="implementation">no_gap</field>
      </record>
    </data>
</odoo>